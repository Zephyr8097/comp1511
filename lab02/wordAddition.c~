#include <stdio.h>
int main()
{
    int i,p,q;
    printf("Please enter two integers: ");
    scanf("%d %d",&i,&p);
    q = i + p;

    if(i == 0) {
        printf("zero");
    } else if(i == 1) {
        printf("one");
    } else if(i == 2){
        printf ("two");
    } else if(i == 3){
        printf("three");
    } else if(i == 4){
        printf("four");
    } else if(i == 5){
        printf("five");
    } else if(i == 6){
        printf("six");
    } else if(i == 7){
        printf("seven");     
    } else if(i == 8){
        printf("eight");     
    } else if(i == 9){
        printf("nine");     
    } else if(i == 10){
        printf("ten");     
    } else if(i == -1){
        printf("negative one");    
    } else if(i == -2){
        printf("negative two");    
    } else if(i == -3){
        printf("negative three");    
    } else if(i == -4){
        printf("negative four");    
    } else if(i == -5){
        printf("negative five");   
    } else if(i == -6){
        printf("negative six");    
    } else if(i == -7){
        printf("negative seven");    
    } else if(i == -8){
        printf("negative eight");    
    } else if(i == -9){
        printf("negative nine");    
    } else if(i == -10){
        printf("negative ten");  
    } else printf("%d",i);
    
    printf(" + ");
    if(p == 0) {
        printf("zero");
    } else if(p == 1) {
        printf("one");
    } else if(p == 2){
        printf ("two");
    } else if(p == 3){
        printf("three");
    } else if(p == 4){
        printf("four");
    } else if(p == 5){
        printf("five");
    } else if(p == 6){
        printf("six");
    } else if(p == 7){
        printf("seven");     
    } else if(p == 8){
        printf("eight");     
    } else if(p == 9){
        printf("nine");     
    } else if(p == 10){
        printf("ten");     
    } else if(p == -1){
        printf("negative one");    
    } else if(p == -2){
        printf("negative two");    
    } else if(p == -3){
        printf("negative three");    
    } else if(p == -4){
        printf("negative four");    
    } else if(p == -5){
        printf("negative five");   
    } else if(p == -6){
        printf("negative six");    
    } else if(p == -7){
        printf("negative seven");    
    } else if(p == -8){
        printf("negative eight");    
    } else if(p == -9){
        printf("negative nine");    
    } else if(p == -10){
        printf("negative ten");
    } else printf("%d",p);
    
    printf(" = ");
    
    if(q == 0) {
        printf("zero\n");
    } else if(q == 1) {
        printf("one\n");
    } else if(q == 2){
        printf ("two\n");
    } else if(q == 3){
        printf("three\n");
    } else if(q == 4){
        printf("four\n");
    } else if(q == 5){
        printf("five\n");
    } else if(q == 6){
        printf("six\n");
    } else if(q == 7){
        printf("seven\n");     
    } else if(q == 8){
        printf("eight\n");     
    } else if(q == 9){
        printf("nine\n");     
    } else if(q == 10){
        printf("ten\n");     
    } else if(q == -1){
        printf("negative one\n");    
    } else if(q == -2){
        printf("negative two\n");    
    } else if(q == -3){
        printf("negative three\n");    
    } else if(q == -4){
        printf("negative four\n");    
    } else if(q == -5){
        printf("negative five\n");   
    } else if(q == -6){
        printf("negative six\n");    
    } else if(q == -7){
        printf("negative seven\n");    
    } else if(q == -8){
        printf("negative eight\n");    
    } else if(q == -9){
        printf("negative nine\n");    
    } else if(q == -10){
        printf("negative ten\n");
    } else printf("%d\n",q);

    return 0;
}
